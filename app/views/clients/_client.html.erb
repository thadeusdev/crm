<div id="<%= dom_id client %>">
  <p>
    <strong>Name:</strong>
    <%= client.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= client.email %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= client.phone %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= client.address %>
  </p>

  <p>
    <strong>Company name:</strong>
    <%= client.company_name %>
  </p>

  <p>
    <strong>Industry:</strong>
    <%= client.industry %>
  </p>

  <p>
    <strong>Notes:</strong>
    <%= client.notes %>
  </p>

</div>
